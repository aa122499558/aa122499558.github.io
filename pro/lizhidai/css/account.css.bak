@charset "utf-8";
/* CSS Document */
a:hover{
	text-decoration:none;
}
.icons{
	text-indent:-9999em;
}
.sidebar-item{
	padding:20px 10px 15px;
	border-bottom:1px solid #e8ecef;
	overflow:hidden;
	zoom:1;
	text-align:center;
}
.account-status h2{
	width:120px;
	font-size:14px;
	font-weight:bold;
	text-align:left;
	height:30px;
	margin:0 auto;
}
.account-status .auth-opts{
	height:30px;
	width:110px;
	margin:0 auto 15px;
}
.account-status .auth-opts ul{
	width:180px;
}
.account-status .auth-opts li{
	height:30px;
	width:30px;
	float:left;
	display:inline;
	margin-right:10px;
}
.button-un-name{
	background-position:-41px -31px;
}
.button-un-tels{
	background-position:-72px -31px;
}
.button-un-pwds{
	background-position:-103px -31px;
}
.account-status .auth-rate{
	height:10px;
	width:110px;
	font-size:0;
	line-height:0;
	margin:0 auto 10px;
	background:#e8ecef;
	border-radius:15px;
}
.account-status .auth-rate div{
	background:#18b160;
	height:10px;
	float:left;
	width:10%;
	border-radius:15px;
}
.account-status .auth-level em{
	color:#18b160;
	padding-right:10px;
}
.balances{
	font-size:16px;
	height:140px;
}
.balances h2{
	color:#333;
	font-weight:bold;
}
.balances h3{
	color:#15456f;
	font-weight:bold;
}
.balances .recharge{
	height:45px;
	padding-top:5px
}
.balances .button-recharge{
	width:60px;
	font-size:16px;
	height:30px;
	line-height:28px;
	text-align:left;
	margin:0 auto;
	padding:0 0 0 50px;
	color:#fff;
}
.balances .recharge .dj{
	width:80px;
	padding:0 15px;
	margin-top:10px;
}
.balances .button-rmb{
	position:absolute;
	left:50%;
	top:5px;
	margin-left:-30px
}
.button-rmb{
	background-position:-41px -94px;
	height:20px;
	width:20px
}
.apply-menu{
	overflow:hidden;
	padding:10px 0 10px 0;
}
.apply-menu dt{
	height:40px;
	line-height:40px;
	padding-left:15px;
}
.apply-menu dd{
	height:30px;
	line-height:30px;
	padding:5px;
	font-size:14px
}
.apply-menu dd.current{
	background:#f0f4f7
}
.apply-menu dd a{
	display:block;
	width:100%;
	text-indent:60px;
	position:relative;
	color:#666
}
.apply-menu dd.current a, .console-menu dd.current a:visited,.apply-menu dd a:hover{
	color:#349cd8	
}

.nav-ico{
	width:16px;
	height:16px;
	font-size:0;
	text-indent:-9999em;
	display:inline-block;
	background:url(../images/nav.png) no-repeat
}
.apply-menu dd .nav-ico{
	position:absolute;
	left:32px;
	top:7px;
}
.apply-menu dd.current .nav-account, .apply-menu a:hover .nav-account{
	background-position:-20px 0;
}
.nav-warn{
	background-position:0 -20px
}
.apply-menu a:hover .nav-warn{
	background-position:-20px -20px
}
.nav-cash{
	background-position:0 -140px;
}
.apply-menu dd.current .nav-cash, .apply-menu a:hover .nav-cash{
	background-position:-20px -140px;
}
.apply-menu dd .nav-card{
	top:10px;
	background-position:0 -40px;
}
.apply-menu dd.current .nav-card, .apply-menu a:hover .nav-card{
	background-position:-20px -40px;
}
.nav-chart{
	background-position:0 -80px;
}
.apply-menu dd.current .nav-chart, .apply-menu a:hover .nav-chart{
	background-position:-20px -80px
}
.nav-invest{
	background-position:0 -60px;
}
.apply-menu dd.current .nav-invest, .apply-menu a:hover .nav-invest{
	background-position:-20px -60px
}
.nav-redeem{
	background-position:0 -100px;
}
.apply-menu dd.current .nav-redeem, .apply-menu a:hover .nav-redeem{
	background-position:-20px -100px;
}
.nav-deal{
	background-position:0 -120px;
}
.apply-menu dd.current .nav-deal, .apply-menu a:hover .nav-deal{
	background-position:-20px -120px;
}
.nav-borrowed{
	background-position:0 -193px;
}
.apply-menu dd.current .nav-borrowed, .apply-menu a:hover .nav-borrowed{
	background-position:-20px -193px;
}


/*main*/
#main{
	margin:0 20px 0 190px;
	overflow:hidden;
}
.title-items{
	position:relative;
	height:70px;
	overflow:hidden;
}
.title-items h2,.title-items .r{
	font-size:16px;
	height:30px;
	line-height:30px;
	color:#69737b;
	position:absolute;
	left:20px;
	top:20px;
	z-index:3;
	padding:0 15px;
	background:#fff;
}
.title-items .r{
	font-size:14px;
	color:#8a949c;
	left:auto;
	right:30px;
	padding-right:0;	
}
.title-items .line{
	height:1px;
	font-size:0;
	line-height:0;
	float:left;
	position:absolute;
	left:0;
	top:35px;
	background:#e7ecee;
	width:100%;
}
.stats-items{
	background:#f0f4f7;
	overflow:hidden;
	border:none;
	height:110px;
	width:100%;
}
.stats-items .items{
	width:100%;
	height:70px;
	overflow:hidden;
	padding:20px 0;
	margin-left:3px;
	position:relative;
}
.stats-entry{
	width:25%;
	float:left;
	display:inline;
	height:70px;
	font-size:14px;
	margin-left:-2px;
	border-left:1px solid #d0d5d9;
	text-align:center;
}
.stats-entry dt{
	position:relative;
	height:20px;
	padding-top:3px;
	margin:0 2px 0 1px;
	color:#8a949c
}
.stats-entry b{
	margin-left:-15px;
	font-weight:normal;
}
.yhelp{
	background-position:-120px -98px;
	width:12px;
	height:
}
.stats-entry dt .yhelp, .stats-entry dd .yhelp{
	position:absolute;
	left:auto;
	top:7px;
	margin-left:3px;
}
.stats-entry dd{
	margin: 0 2px 0 1px;
	height:40px;	
}
.stats-entry dd strong{
	font-size:24px;
	display:block;
	height:30px;
	overflow:hidden;
	font-family:Tahoma;
	font-weight:normal;
}
.stats-entry dd p{
	position:relative;
	height:24px;
	line-height:24px;
	overflow:hidden;
}
.light-gray{
	color:#8a949c;	
}
.stats-entry ins{ 
	text-decoration:none;
	font-family:Microsoft YaHei, Helvetica, Tahoma;	
	
}
.stats-items2{
	height:250px;
}
.items2-left{
	width:600px;
	height:180px;
	background:none;
	float:left;
	margin:15px 0 0 15px;
	border-bottom:1px dashed #ccc
	
}
.items2-right{
	width:150px;
	height:150px;
	background:#f0f4f7;
	float:right;
	margin:45px 15px 0 0;
}
.items2-right h2{
	font-size:18px;
	font-weight:bold;
	line-height:150px;
	text-align:center
}
.stats-items2 ul li{
	float:left;
	width:120px;
	height:180px;
	position:relative;
}
.stats-items2 .money{
	position:absolute;
	font-size:12px;
	bottom:3px;
}
.stats-items2 .items-title{
	font-size:16px;
	position:absolute;
	bottom:-26px;	
}
.stats-items2 .columnar{
	width:30px;
	height:50px;
	position:absolute;
	left:50px;
	bottom:0;
	background:#f0f4f7;
}
.record-title{
	background:#e8ecef;
	overflow:hidden;
	position:relative;
}
.record-title h2{
	text-align:center;
	line-height:34px;
}
.record-title a{
	position:absolute;
	right:10px;
	top:5px;
}
.record-items{
	height:200px;
}

/*bankCards*/
.bankcards-wrap{
	overflow:hidden;
	zoom:1;
}
.bank-card{
	width:375px;
	height:230px;
	float:left;
	display:inline;
	margin-top:30px;
	position:relative;
}
.bank-card-add{
	overflow:visible;
	height:auto;
	background:#fff
}
.bank-add-button{
	border-top:1px solid #d3d8dc;
	border-bottom:1px solid #d3d8dc;
	height:130px;
	margin:49px 0;
	text-align:center;
}
.bank-add-button a{
	position:relative;
	color:#349cd8;
	margin-top:55px;
	display:inline-block;
	font-size:14px;	
}

.bank-add-button .add-blue{
	position:absolute;
	left:0;
	top:4px;
	margin-left:-20px;
}
.bank-card-opt{
	display:none;
}
.bank-card-opt .head{
	border-bottom:1px solid #d3d8dc;
}
.bank-card .title{
	text-align:center;
	padding-top:15px;
	position:relative;
}
.bank-card .head, .bank-card .foot{
	background:#e7ecee;	
	height:50px;
	position:relative;
}
.add-gray{
	background-position:-72px -113px;
	width:14px;
	height:14px;	
}
.bank-card .title .icons{
	position:absolute;
	left:32%;
	top:18px;
}
.bank-card-opt .bank-items{
	padding:18px 0 10px;
}
.bank-card-opt .bank-items li{
	height:48px;
	overflow:hidden;
	position:relative;
}
.bank-card-opt .bank-items label{
	float:left;
	display:inline;
	line-height:33px;
	margin-right:5px;
	width:80px;
	text-align:right
}
.bank-card-opt .bank-items input{
	width:220px;
	height:19px;
	line-height:19px;
	padding:6px 5px;
	color:#999
}
.bank-card-opt .bank-select{
	color:#349cd8;
	position:absolute;
	right:53px;
	top:5px;
	cursor:pointer;
	padding-right:10px;
	height:20px;
	float:right;
	line-height:20px;
}
.arrow-blue-down{
	background-position:-50px -134px;
	width:9px;
	height:5px;
}
.bank-card-opt .bank-select .icons{
	position:absulute;
	right:0;
	top:8px;x	
}
.bank-card-opt .Error{
	color:#d35353;
	padding-left:85px
}
.bank-card-opt .foot{
	border-top:1px solid #d3d8dc;
}
.bank-card .foot .bank-action{
	float:left;
	position:relative;
	left:50%;
	padding-top:13px
}
.bank-card .bank-delete{
	background:#fff;
	color:#8a949c;
}
.bank-card .foot a{
	padding:0 15px 0 25px;
	height:26px;
	line-height:26px;
	margin-right:10px;
	float:left;
	display:inline;
	white-space:nowrap;
	position:relative;
	left:-50%;
	overflow:hidden;
}
.bank-card .bank-delete:visited{
	color:#8a949c;
}
.bank-card .bank-bind{
	background:#15456f;
	color:#fff
}
.yclose{
	background-position:-68px -77px;
	width:8px;
	height:8px;
}
.cm-white{
	background-position:-105px -114px;
	width:10px;
	height:8px;
}
.bank-card .foot .icons{
	position:absolute;
	left:13px;
	top:10px;
}

/*information*/
.information table{
	border:1px solid #b6cae5;
}
.information table td,.information table th{
	text-align:center;
	padding:5px;
}
.information table th,.information table td{
	border-bottom:1px solid #b6cae5;
	border-right:1px solid #b6cae5
}
.information table tr:first-child{
	background:#f2f5fa
}
.information table tr:last-child{
	background:#f2f5fa
}
.information table .sender{
	display:block;
	width:24px;
	height:24px;
	background-position:-565px -100px;
	margin:0 auto
}
.information .td1{
	width:60px;
}
.information .td2{
	width:60px;
}
.information .td3{
	width:150px;
}
.information .td4{
	width:300px;
}
.information .td5{
	width:150px;
	font-size:12px;
	color:#555;
}
.information .td6{
	padding-left:50px
}
.information .td6 li{
	float:left;
	margin-left:12px;
}
.information .td6 li a{
	color:#555;
}
.information .td6 li a:hover{
	color:#349cd8;
}

/*attention-setting*/
.setting table tr:first-child,.setting table tr:last-child{
	height:50px
}
.setting table td{
	width:145px !important;
}
.setting table .btn-sub{
	display:inline-block;
	padding:0 15px;
	height:30px;
	line-height:30px;
	background:#1a588e;
	color:#fff;
	border-radius:3px;
	float:right
}
.setting table .cancel{
	background:none;
	padding:0 25px;
	color:#349cd8;
}

/*recharge*/
.excel-export a, .excel-export a:visited{
	color:#475058
}

.cash-top{
	height:90px;	
}
.recharge-entry{
	width:25%;
	float:left;
	display:inline;
	height:55px;
	font-size:14px;
	margin-left:-2px;
	border-left:1px solid #d0d5d9;
	text-align:center;
}
.recharge-entry dt{
	position:relative;
	line-height:narmal;
	font-size:20px;
}
.recharge-entry dd{
	position:relative;
	height:24px;
	line-height:24px;
	overflow:hidden;	
}
.recharge-entry dd .yhelp{
	position:absolute;
	left:auto;
	top:6px;
	margin-left:3px;
}
.recharge-entry dt strong{
	font-size:24px;
	font-family:Tahoma, Geneva, sans-serif;
	font-weight:normal;	
}
.recharge-entry ins{
	font-family:Microsoft YaHei, Helvetica, Tahoma;
	font-style:normal;
	text-decoration:none;	

}
.tl{
	text-align:left;	
}
.tl dt{
	padding-left:20px;	
}
.tl dd{
	padding-left:25px;	
}
.submit-rw{
	position:absolute;
	right:30px;
	top:50%;
	height:50px;
	margin-top:-24px;	
}
.submit-rw a{
	position:relative;
	color:#fff;
	width:125px;
	height:50px;
	line-height:50px;
	margin-left:20px;
	text-indent:55px;
	padding:0;
	font-size:16px;	
}
.submit-rw .recharge{
	background:#15456f;	
}
.submit-rw .withdraw{
	background:#18b160;
}
.button-rmb{
	background-position:-41px -94px
	height:20px;
	width:20px;
}
.button-wit{
	background-position:-54px -115px;
	height:13px;
	width:16px;	
}
.submit-rw .icons{
	position:absolute;
	left:50%;
	top:50%;
	margin:-10px 0 0 -32px;	
}
.submit-rw .withdraw .icons{
	margin:-6px 0 0 -30px;	
}
.trade-filter{
	padding-bottom:10px;	
}
.filter-items{
		overflow:hidden;
		zoom:1;
		padding-top:10px;
		margin:0 40px;
		line-height:30px;
}
.filter-items dt{
	width:65px;
	float:left;
	display:inline;	
}
.filter-items dd{
	margin-left:80px;
	height:30px;	
}
.query-input{
	float:left;
	display:inline;
	padding-right:20px;
	height:30px;	
}
.query-input input{
		height:18px;
		line-height:18px;
		width:90px;
		padding:6px 0 5px 35px;
		padding:6px 0 5px 25px\9;
		text-indent:10px\9;
		font-family:Tahoma;
		border:0;
		background:#f0f4f7 url(../images/ui-date.png) no-repeat 10px 9px;
		border-radius:3px;
		float:left;
		display:inline;
		overflow:hidden;
		cursor:default;
}
.query-input label{
	padding:0 5px;
	float:left;
	display:inline;	
}
.filter-items dd a{
	display:inline-block;
	padding:0 10px;
	height:24px;
	line-height:24px;
	margin:4px 10px 0 0;
	float:left;
	border-radius:3px;
		
}
.filter-items a:hover{
	color:#fff;
	background:#15456f;	
	cursor:pointer
}
.filter-items a.selected, .filter-items a.selected:visited{
	color:#fff;
	cursor:default;
	background:#15456f;	
}
.trade-table{
	overflow:hidden;
	zoom:1;
	font-size:12px;	
}
.trade-table table{
	width:100%;
	text-align:left;
	line-height:1.6em;
}
.trade-table th, .trade-table td{
		padding:15px 5px 15px 43px;
}
.trade-table th{
	background:#e8ecef;
	vertical-align:middle;
	text-align:left;
	font-weight:bold;
	font-size:13px	
}