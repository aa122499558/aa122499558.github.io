@charset "utf-8";
/* CSS Document */
body{
	background:#f7f7f7;
	font:13px/1.5 Microsoft YaHei,Helvetica,Tahoma;
}
a{
	color:#349cd8;
	text-decoration:none;
}
a:hover{
	text-decoration:underline;
}
a:visited{
	color:#349cd8;
}
input.input{
	height:18px;
	line-height:18px;
	width:240px;
	padding:11px 5px;
	border:1px solid #b4b9bd;
	border-radius:3px;
}

.none{
	display:none;
}

.icons{
	background:url(../images/sprites.png) no-repeat;
}
.fl{
	float:left	
}
#header{
	height:140px;
	position:relative;
	margin-bottom:10px;
}
#header .topbar{
	height:40px;
	background:#256f9a;
	position:absolute;
	top:0;
	width:100%;
	min-width:1000px;
	z-index:1;
}
#header .inner, #footer .inner{
	height:140px;
	width:1000px;
	margin:0 auto;
	position:relative;
	z-index:5;
}
#header .service{
	height:40px;
	position:absolute;
	left:10px;
	top:0px;
	line-height:40px;
	color:#8a949c;	
}
#header .service b{
	float:left;
	display:inline-block;	
}
#header .hotline em{
	font-family:Tahoma;
	font-size:20px;
	padding-left:5px;
	float:left;
	display:inline-block;
	line-height:normal;
	margin-top:6px;
}
#header .service li{
	float:left;
	display:inline;
	height:40px;
	padding-left:20px;	
}
#header .hotline .icons{
	background-position:-191px -146px;
	width:14px;
	height:14px;
	position:absolute;
	left:0;
	top:14px;
}
#header .concerns{
	color:#8a949c;
}
#header .weixin{
	background-position:-206px -219px;
	width:19px;
	height:14px;
	float:left;
	display:inline-block;
	margin:13px 0 0 10px;;
}
#header .weixin:hover{
	background-position:-206px -201px
}
.loginbar{
	height:40px;
	line-height:40px;
	position:absolute;
	right:10px;
	top:0;
}
.loginbar a{
	display:inline-block;
	margin-left:20px;	
}
#header .logo{
	width:370px;
	height:51px;
	overflow:hidden;
	position:absolute;
	left:30px;
	top:70px;
}
#header .logo h1{
	text-indent:-9999em;
}
#header .nav-wrapper{
	width:520px;
	height:40px;
	font-size:16px;
	line-height:40px;
	position:absolute;
	top:80px;
	right:10px;	
}
#header .nav-wrapper .nav{
	height:40px;
	float:left;
	display:inline-block;
	border-right:1px solid #fff;	
}
#header .nav-wrapper .nav li{
	float:left;
	display:inline-block;
	padding:0 20px;	
}
#header .nav-wrapper a{
	color:#475058
}
#header .nav-wrapper a:hover{
	color:#349cd8
}
#header .nav-wrapper .use{
	float:left;
	height:40px;
	display:inline-block;
	border-left:1px solid #c0c9d0;
}
#header .nav-wrapper .use a{
	float:left;
	display:inline-block;
	margin-left:20px;
	text-align:center;
	width:94px;
	height:40px;
	background:url(../images/sprites.png) no-repeat -97px -260px
}
#header .nav-wrapper .use a:hover{
	text-decoration:none;
	background-position:0 -260px;
	color:#fff;
}
#wrapper,#wrap{
	width:1000px;
	margin:0 auto;
	overflow:hidden;
	background:#fff;
}
#wrap{
	background:none
}
.dot24{
	background-position:-157px -71px;
	width:24px;
	height:24px;
}
.gbtn, .gbtn-disabled, .gbtn-org{
	position:relative;
	white-space:nowrap;
	background:#15456f;
	display:inline-block;
	padding:3px 5px;
	cursor:pointer;
	overflow:hidden;
	font-family:Microsoft YaHei;
}

/*footer*/
#footer{
	height:230px;
	position:relative;
	margin-top:20px
}
.fool{
	height:230px;
	float:left;
	display:inline-block;
	width:59%;
}
#footer .navsub{
	height:50px;
	padding:20px 0 0 15px;
	color:#fff;
}
#footer .navsub li{
	float:left;
	display:inline-block;
	padding-right:15px;
}
#footer .foor{
	height:230px;
	float:right;
	width:40%;
}
#footer .hotl{
	height:120px;
	padding-top:20px;
}
#footer .footer2 .hotl{
	color:#475058;
	padding-top:10px;
}
#footer .hotl em{
	font-size:30px;
	font-family:Tahoma,Geneva,sans-serif;
	line-height:2.1em;
}
#footer .online{
	display:inline-block;
	position:relative;
	padding-right:20px;
	margin-left:15px;
}
#footer .hotl .online .icons{
	width:14px;
	height:14px;
	position:absolute;
	right:0;
	top:3px;
	background-position:-205px -242px;
}
#footer .cabout{
	height:70px;
	position:relative;
	padding-left:15px;
	color:#fff;
}
#footer .cabout h2{
	float:left;
	display:inline-block;
	padding-top:15px;
}
#footer .cabout li{
	float:left;
	display:inline-block;
	width:48px;
	height:48px;
	margin-left:30px;
	position:relative;
}
#footer .cabout a{
	display:block;
	width:48px;
	height:48px;
	border-radius:50%;
	position:absolute;
	left:0;
	top:0;
	transition:all 0.6s cubic-bezier(0.34,0.67,0.17,0.95);
}
#footer .cabout a .icons{
	position:absolute;
	left:50%;
	top:50%;
	text-indent:-9999em;
}
#footer .cabout a.weixin .icons{
	width:33px;
	height:25px;
	margin:-13px 0 0 -16px;
	background-position:-268px -88px;
}
#footer .letter{
	height:38px;
	padding-top:16px;
}
#footer .b,#footer .t{
	background:#475058;
	height:90px;
	top:140px;
	position:absolute;
	left:0;
	width:100%;
	min-width:1000px;
}
#footer .t{
	background:#256f9a;
	top:0;
	height:140px;
	z-index:1;
}
#footer .read{
	fong-size:12px;
	padding-top:5px;
	color:#8a949c;
}
.yhelp{
	background-position:-120px -98px;
	width:12px;
	height:12px;
	text-indent:-9999em;
}
.r3, .gbtn, .model-box, .dialog-main, input, .custom-select{
	border-radius:3px;	
}
#footer .letter a{
	float:left;
	display:inline;
	margin-right:30px;
}
#footer .letter .yx{
	width:28px;
	height:28px;
	background-position:-277px 0;
}
#footer .letter .yx:hover{
	background-position:-277px -30px;
}
#footer .letter .yn{
	width:60px;
	height:32px;
	margin-top:-2px;
	background-position:-305px 0;
}
#footer .letter .yn:hover{
	background-position:-305px -39px;
}
#footer .letter .yk{
	width:67px;
	height:26px;
	background-position:-305px -145px
}
#footer .letter .yk:hover{
	background-position:-305px -174px;
}
#footer .letter .yc{
	margin-top:-1px;
	width:75px;
	height:28px;
	background-position:-305px -80px
}
#footer .letter .yc:hover{
	background-position:-305px -112px
}
#footer .letter .yb{
	width:75px;
	margin-top:-1px;
	height:28px;
	background-position:-235px -272px;
}
#footer .letter .yb:hover{
	background-position:-313px -272px;
}
#footer .rights{
	padding-top:5px;
	color:#8a949c
}
#footer .fico{
	height:40px;
	padding:15px 0 0 85px;
	position:relative;
	font-size:12px;
	color:#8a949c;
	line-height:1.4em
}
#footer .fico .icons{
	width:81px;
	height:27px;
	background-position:-305px -206px;
	position:absolute;
	left:0;
	top:18px;
	text-indent:-9999em
}
#footer .fico .icons:hover{
	background-position:-305px -239px;
}
#footer .footer2 .navsub li a{
	color:#475058
}
#footer .footer2 .navsub li a:hover{
	color:#349cd8
}
#footer .footer2 .cabout h2{
	color:#475058
}
#footer .foot-letter{
	padding-top:5px;
}
#footer .foot-letter a{
	margin-right:15px
}
#footer .footer2 .fool,#footer .footer2 .foor{
	height:140px;
}
#footer .footer2 a.weibo .icons{
	width:31px;
	height:26px;
	margin:-13px 0 0 -15px;
	background-position:-268px -60px
}
#footer .footer2 .cabout li{
	background:url(../images/sprites.png) -150px -209px;

}
#footer .footer2 .cabout li:hover{
	background:url(../images/sprites.png) -100px -209px;
}
#footer .footer2 .foor,#footer .footer2 .hotl{
	height:65px;
}
#footer .footer2 .hotl em{
	font-size:18px;
}
/*footer*/
#footer .inner{
	
}
#footer .navsub, #footer .cabout, #footer .hotl, #footer .hotl .online, #footer .navsub a, #footer .navsub a:visited{
	color:#fff;
}
#footer .cabout a{
	background-color:#143f64;
}
#footer .cabout a.weibo .icons{
	width:31px;
	height:26px;
	margin:-13px 0 0 -15px;
	background-position:-268px -60px;
}

/*account*/
#sidebar,#mian{
	min-height:750px;
}
#sidebar{
	width:170px;
	float:left;
	display:inline;
	border-right:1px solid #e8ecef;
}
.gbtn:hover{
	background:#1a588e;
	text-decoration:none;
	color:#fff;
}
.add-blue{
	background-position:-88px -113px;
	width:14px;
	height:14px;
}
.model-box{
	border:1px solid #d3d8dc;
	margin-bottom:20px;
}